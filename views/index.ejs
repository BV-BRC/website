<!DOCTYPE html>
<html>

<head>
  <% include header %>
</head>

<body onunload="" class="claro patric patric_body <% if (request.isAuthenticated && request.isAuthenticated()) { %>Authenticated<% } %>">

  <!-- New Navbar, CSS scoped to .patric_bootstrap -->
  <div class="patric_bootstrap">
    <% include navbar %>
  </div>

  <div id="ApplicationContainer" data-dojo-type="dijit/layout/BorderContainer" class="layoutContainer"
    data-dojo-props="id:'ApplicationContainer',gutters:false,liveSplitters:false" style="height:calc(100% - 38px)">
    <!-- start application container -->

    <div data-dojo-type="dijit/layout/ContentPane" data-dojo-props="region:'center'" style="padding: 0px;">

      <a name="top"></a>
      <!-- start contentpane container -->
      <% include home %>

    </div><!-- end contentpane container -->

    <% if (request.isAuthenticated && request.isAuthenticated()) { %>
    <div data-dojo-type="p3/widget/WorkspaceController" data-dojo-props="region:'bottom'"></div>
    <% } %>

  </div><!-- end application container -->

  <% include javascript %>
</body>

</html>